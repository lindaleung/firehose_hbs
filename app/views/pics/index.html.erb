<% @pics.each do |pic| %>
	<div class="span3 offset0 boxy text-center">
		<h1 class="h1-custom">
			Lesson: <%= pic.lesson %>
		</h1>

		<h2>
			<br>
			Photo: <%= image_tag pic.image %>	
			<br>
			Emotion: <%= pic.emotion %>
			<br>
			Learned: <%= pic.learned %>
			<br>
		</h2>
	</div>

<% end %>